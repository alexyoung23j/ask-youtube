{
  "name": "ask-youtube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "yarn --cwd prisma prisma db push && next dev",
    "start-cloud-functions": "concurrently \"tsc --watch --project google-cloud\" \"nodemon --delay 2 --watch google-cloud/build/ --exec 'npx @google-cloud/functions-framework --source=google-cloud/build/ --target=transcriptionJob --port=3001'\"",
    "deploy-functions": "cd google-cloud && yarn && tsc && cp package*.json build/ && gcloud functions deploy transcriptionJob --runtime nodejs18 --trigger-http --source build/ --entry-point transcriptionJob",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start-prod": "next start"
  },
  "dependencies": {
    "@deepgram/sdk": "^2.1.1",
    "@google-cloud/functions-framework": "^3.2.0",
    "@google-cloud/pubsub": "^3.7.1",
